                                <img style="width: 212px; height: 307px;"  src="{{ asset("bundles/stageapp/images/recto-11593.png") }} " alt="item1" class="item-image">
                                <img style="width: 212px; height: 307px;" src="{{ asset("bundles/stageapp/images/recto-11593-hover.png") }} " alt="item1  Hover" class="item-image-hover">
