                        </div><!-- End .item-action -->
                    </div><!-- End .item-meta-container -->
                </div><!-- End .item -->
            </div>
            </div>
    </div>
    </div>

{% endblock %}




{% block footer %}

    <div id="globBot">
        {% set footer = app.session.get('f') %}
        <div id="site-footers">
            {% if footer.Id == 1 %}
                {{ include('StageappBundle:Footers:f1.html.twig') }}
            {% endif %}

            {% if footer.Id == 2 %}
                {{ include('StageappBundle:Footers:f2.html.twig') }}
            {% endif %}
        </div>
    </div>

{% endblock %}


