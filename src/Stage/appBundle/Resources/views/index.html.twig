            </ul>
            <div id="option-panel-title" class="clearfix">
                <span>Footers</span>
                <a href="#" id="option-panel-save">Save</a>
                <a href="#" id="option-panel-reset">Reset</a>
            </div>
            <div id="option-panel-content" class="tab-content">
                <div class="tab-pane fade in active" id="panel-footers">
                    <div id="panel-option-footer" class="panel-group custom-accordion sm-accordion">
                        <div class="panel">
                            <div class="accordion-header">
                                <div class="accordion-title"><span>Footers Chooser</span></div><!-- End .accordion-title -->
                                <a class="accordion-btn opened"  data-toggle="collapse" data-target="#header-mode" data-parent="#panel-option-footers"></a>
                            </div><!-- End .accordion-header -->

                            <div class="form-group colorbox-list clearfix collapse in" id="header-mode">
                                <label for="sel2">Select Footer:</label>
                                <select class="form-control" id="sel2">
                                    <option value="1">Footer 1</option>
                                    <option value="2">Footer 2</option>
                                </select>
                            </div>
                        </div><!-- End .panel -->

                    </div><!-- .panel-group -->
                </div><!-- End .tab-pane -->
                <div class="tab-pane fade" id="panel-home">
                    <div id="panel-home-accordion" class="panel-group custom-accordion sm-accordion">
                        <div class="panel">
                            <div class="accordion-header">
                                <div class="accordion-title"><span>Basic Color</span></div><!-- End .accordion-title -->
                                <a class="accordion-btn opened"  data-toggle="collapse" data-target="#body-background-color" data-parent="#panel-home-accordion"></a>
                            </div><!-- End .accordion-header -->

                            <div id="body-background-color" class="collapse in">
                                <div class="panel-body">
