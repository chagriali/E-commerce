<script src="{{ asset('bundles/stageapp/js/owl.carousel.min.js') }}"></script>
<script src="{{ asset('bundles/stageapp/js/jflickrfeed.min.js') }}"></script>
<script src="{{ asset('bundles/stageapp/js/jquery.prettyPhoto.js') }}"></script>
<script src="{{ asset('bundles/stageapp/js/jquery.sequence-min.js') }}"></script>
<script src="{{ asset('bundles/stageapp/js/colpick.js') }}"></script>
<script src="{{ asset('bundles/stageapp/js/script.js') }}"></script>

<script>
    $(function() {
        // Sequence.js Slider Plugin
        var options = {
                    nextButton: true,
                    prevButton: true,
                    pagination:true,
                    autoPlay: true,
                    autoPlayDelay: 8500,
                    pauseOnHover: true,
                    preloader: true,
                    theme: 'slide',
                    speed: 700,
                    animateStartingFrameIn: true
                },
                homeSlider = $('#slider-sequence').sequence(options).data("sequence");


        $( "#sel1" ).change(function() {
            var i = $("#sel1").val();





          $("#glob").append( "<div id=\"site-header\" ></div>");
            $.ajax({url: 'http://localhost:82/HySymfony/web/app_dev.php/' + '{{ app.request.get('_locale') }}'   + '/ajax-header/' + i , success: function(result){
                $( "#site-header" ).remove();
                $("#site-header").html(result);
                var stylesheet ;
                switch(i){
                    case '1' :
                        stylesheet =  "{{ asset('bundles/stageapp/css/headers/h1.css') }}";

                        break;
                    case '2' :
                        stylesheet =  "{{ asset('bundles/stageapp/css/headers/h2.css') }}";break;

                    case '3' :
                        stylesheet =  "{{ asset('bundles/stageapp/css/headers/h3.css') }}";break;

                }

                $("#header-css").attr("href", stylesheet);


                // Sub menu show/hide with hoverIntent plugin
                if ($.fn.hoverIntent) {

                    $('ul.menu').hoverIntent(function() {
                                $(this).children('ul, .mega-menu').fadeIn(100);

                            }, function() {
                                $(this).children('ul, .mega-menu').fadeOut(50);
                            },
                            'li');

                } else {

                    $('ul.menu').find('li').mouseover(function() {
                        $(this).children('ul, .mega-menu').css('display','block');

                    }).mouseout(function() {
                        $(this).children('ul, .mega-menu').css('display','none');
                    });
                }



            }});
        });


        $( "#sel2" ).change(function() {
            var i = $("#sel2").val();



            $("#globBot").append( "<div id=\"site-footers\" ></div>");
            $.ajax({url: 'http://localhost:82/HySymfony/web/app_dev.php/'+ {{ app.session.get('loc') }}  + '/ajax-footer/' + i  , success: function(result){
                $( "#site-footers" ).remove();
                $("#site-footers").html(result);

                var stylesheet;
                switch(i){
                    case '1' :
                        stylesheet =  "{{ asset('bundles/stageapp/css/footer/f1.css') }}";

                        break;
                    case '2' :
                        stylesheet =  "{{ asset('bundles/stageapp/css/footer/f2.css') }}";break;
                }



                $("#footer-css").attr("href", stylesheet);

            }});
        });

    });

</script>


</body>
</html>